package day6;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;


public class MainAppDay6 {

	public static void main(String[] args) {
		ApplicationContext context = new ClassPathXmlApplicationContext("beans_day6_p_namespace.xml");
		Employee swimmingPool = (Employee)context.getBean("em");
		System.out.println("--------- Day 6: P -------------");
		swimmingPool.getInfomationEmployee();

	}

}

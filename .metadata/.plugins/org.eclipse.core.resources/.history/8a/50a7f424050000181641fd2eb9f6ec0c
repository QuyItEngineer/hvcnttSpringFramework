		<!-- <property name="nameCustomer" value="Nguyen Van A"></property> <property 
			name="ageCustomer" value="25"></property> <property name="sexCustomer" value="Nam"></property> 
			<property name="idCustomer" value="123456789"></property> -->

		<!-- Inject by Constructor -->
		<constructor-arg value="Nguyen Van A" />
		<constructor-arg value="24" />
		<constructor-arg value="Nam" />
		<constructor-arg value="123456789" />
		<constructor-arg>

			<ref bean="address" /> <!-- ex2: can write a bean id="address" in here -->

